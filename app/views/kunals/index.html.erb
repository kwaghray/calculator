<h1>Listing kunals</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Decription</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @kunals.each do |kunal| %>
  <tr>
    <td><%= kunal.name %></td>
    <td><%= kunal.title %></td>
    <td><%= kunal.decription %></td>
    <td><%= link_to 'Show', kunal %></td>
    <td><%= link_to 'Edit', edit_kunal_path(kunal) %></td>
    <td><%= link_to 'Destroy', kunal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

